{
    "name": "tbence/validate",
    "description": "Automatic validation for laravel models.",
    "keywords": ["validation", "laravel", "model"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bence Tihanyi",
            "email": "tihanyibence@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "TBence\\Validate\\": "src/"
        }
    },
